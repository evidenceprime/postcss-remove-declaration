{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "strict": true,
    "moduleResolution": "node",
    "lib": [
      "es2019"
    ],
    "sourceMap": false,
    "outDir": ".",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "declaration": true,
    "skipLibCheck": true
  },
  "include": [
    "index.ts"
  ],
  "exclude": []
}
